{
  "name": "us-house",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "test": "",
    "build:js:compile": "rollup -c",
    "build:js:minify": "pushd ../output/ && uglifyjs compiled.js -o compiled.min.js -c -m --source-map content=compiled.js.map,url=compiled.min.js.map && popd",
    "build:js": "npm run build:js:compile && npm run build:js:minify",
    "build:css": "node-sass --output-style=compressed --source-map=true css/main.sass ../output/main.css",
    "build": "cp index.html ../output/ && npm run build:js && npm run build:css",
    "watch:js": "onchange 'js/*.js' -- npm run build:js",
    "watch:css": "onchange 'css/*.sass' -- npm run build:css",
    "watch:html": "onchange 'index.html' -- cp index.html ../output/",
    "watch": "npm run watch:js & npm run watch:css & npm run watch:html & ",
    "start": "cd ../output/ && python3 -m http.server",
    "dev": "npm run watch & npm start",
    "clean": "rm ../output/*.js && rm ../output/*.map &&  rm ../output/main.css"
  },
  "repository": "https://github.com/CoryMcCartan/us-house",
  "author": "Cory McCartan",
  "license": "ISC",
  "devDependencies": {
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "dependencies": {
    "d3": "^4.9.1"
  }
}
